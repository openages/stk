{
      "name": "@openages/craftkit",
      "version": "1.2.0",
      "description": "The base kit for your craft.",
      "author": "Wendao",
      "license": "MIT",
      "homepage": "https://github.com/openages/craftkit",
      "repository": {
            "type": "git",
            "url": "git+https://github.com/openages/craftkit.git"
      },
      "exports": {
            "./react": {
                  "default": "./react/index.js",
                  "types": "./react/index.d.ts"
            },
            "./storage": {
                  "default": "./storage/index.js",
                  "types": "./storage/index.d.ts"
            },
            "./types": {
                  "default": "./types/index.js",
                  "types": "./types/index.d.ts"
            }
      },
      "files": [
            "./react",
            "./storage",
            "./types",
            "./package.json",
            "./README.md"
      ],
      "scripts": {
            "clear": "tsx ./build/clear.ts",
            "move": "tsx ./build/after.ts",
            "dev": "rspack build --watch",
            "build": "pnpm run clear && rspack build && tsc && pnpm run move"
      },
      "keywords": [
            "react",
            "nodejs",
            "web",
            "utils",
            "typescript"
      ],
      "dependencies": {
            "fast-equals": "^5.0.1",
            "react": "^18.2.0"
      },
      "devDependencies": {
            "@rspack/cli": "^0.2.7",
            "ts-node": "^10.9.1",
            "@types/fs-extra": "^11.0.1",
            "@types/node": "^20.4.2",
            "@types/react": "^18.2.15",
            "fast-glob": "^3.3.0",
            "fs-extra": "^11.1.1",
            "tsx": "^3.12.7",
            "typescript": "^5.1.6"
      }
}